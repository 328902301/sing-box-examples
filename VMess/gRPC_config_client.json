{
    "log": {
        "level": "info",
        "timestamp": true
    },
    "route": {
        "rules": [
            {
                "geosite": [
                    "cn",
                    "private"
                ],
                "geoip": [
                    "cn",
                    "private"
                ],
                "outbound": "direct"
            }
        ]
    },
    "inbounds": [
        {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "::",
            "listen_port": 10000
        }
    ],
    "outbounds": [
        {
            "type": "vmess",
            "tag": "proxy",
            "server": "", // 服务端的域名或 IP；域名需确保是包含在服务端的证书中，且已正确指向了服务端的 IP
            "server_port": 443,
            "uuid": "chika",
            "security": "aes-128-gcm",
            "alter_id": 0,
            "global_padding": true,
            "authenticated_length": true,
            "tls": {
                "enabled": true,
                "server_name": "", // 如果 "server" 填的是域名，可留空；如果 "server" 填的是 IP，填服务端的证书中包含的域名，且可不指向服务端的 IP
                "utls": {
                    "enabled": true,
                    "fingerprint": "chrome"
                }
            },
            "packet_encoding": "packetaddr",
            "transport": {
                "type": "grpc",
                "service_name": "lovelive" // 与服务端一致
            }
        },
        {
            "type": "direct",
            "tag": "direct"
        }
    ]
}
