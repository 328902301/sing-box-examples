{
    "log": {
        "level": "info",
        "timestamp": true
    },
    "inbounds": [
        {
            "type": "vless",
            "tag": "vless-in",
            "listen": "::",
            "listen_port": 443,
            "sniff": true,
            "sniff_override_destination": true,
            "domain_strategy": "ipv4_only",
            "users": [
                {
                    "uuid": ""
                }
            ],
            "tls": {
                "enabled": true,
                "server_name": "",
                "reality": {
                    "enabled": true,
                    "handshake": {
                        "server": "",
                        "server_port": 443
                    },
                    "private_key": "",
                    "short_id": [
                        ""
                    ]
                }
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        }
    ],
    "route": {
        "geoip": {
            "path": "geoip.db",
            "download_url": "https://github.com/soffchen/sing-geoip/releases/latest/download/geoip.db"
        },
        "geosite": {
            "path": "geosite.db",
            "download_url": "https://github.com/soffchen/sing-geosite/releases/latest/download/geosite.db"
        },
        "rules": [
            {
                "geosite": [
                    "category-ads-all"
                ],
                "outbound": "block"
            },
            {
                "geoip": [
                    "cn"
                ],
                "outbound": "block"
            }
        ]
    }
}
