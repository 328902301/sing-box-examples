{
    "inbounds": [
        {
            "type": "hysteria",
            "listen": "::",
            "listen_port": 16384,
            "sniff": true,
            "sniff_override_destination": true,
            "domain_strategy": "ipv4_only",
            "up_mbps": 1000,
            "down_mbps": 1000,
            "users": [
                {
                    "auth_str": "chika"
                }
            ],
            "tls": {
                "enabled": true,
                "alpn": [
                    "h3"
                ],
                "certificate_path": "fullchain.cer",
                "key_path": "private.key"
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        }
    ],
    "route": {
        "geoip": {
            "path": "geoip.db",
            "download_url": "https://github.com/SagerNet/sing-geoip/releases/latest/download/geoip.db"
        },
        "rules": [
            {
                "geoip": "cn",
                "outbound": "block"
            }
        ]
    }
}
