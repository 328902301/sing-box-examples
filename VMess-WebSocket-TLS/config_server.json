{
    "log": {
        "level": "info"
    },
    "dns": {
        "servers": [
            {
                "address": "1.1.1.1"
            }
        ]
    },
    "route": {
        "geoip": {
            "path": "geoip.db",
            "download_url": "https://github.com/SagerNet/sing-geoip/releases/latest/download/geoip.db"
        },
        "rules": [
            {
                "geoip": [
                    "cn"
                ],
                "outbound": "block"
            }
        ]
    },
    "inbounds": [
        {
            "type": "vmess",
            "tag": "vmess-in",
            "listen": "127.0.0.1",
            "listen_port": 8001,
            "sniff": true,
            "sniff_override_destination": true,
            "domain_strategy": "ipv4_only",
            "users": [
                {
                    "uuid": "0d74dc01-d920-528a-9a05-cd203e12f587",
                    "alterId": 0
                }
            ],
            "transport": {
                "type": "ws",
                "path": "/lovelive_liella",
                "max_early_data": 0,
                "early_data_header_name": "Sec-WebSocket-Protocol"
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        }
    ]
}
