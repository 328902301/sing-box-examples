{
    "log": {
        "level": "info",
        "timestamp": true
    },
    "route": {
        "geoip": {
            "path": "geoip.db",
            "download_url": "https://github.com/soffchen/sing-geoip/releases/latest/download/geoip.db"
        },
        "geosite": {
            "path": "geosite.db",
            "download_url": "https://github.com/soffchen/sing-geosite/releases/latest/download/geosite.db"
        },
        "rules": [
            {
                "geosite": [
                    "cn"
                ],
                "outbound": "direct"
            },
            {
                "geoip": [
                    "cn",
                    "private"
                ],
                "outbound": "direct"
            }
        ]
    },
    "inbounds": [
        {
            "type": "mixed",
            "listen": "::",
            "listen_port": 10808, // http和socks5监听端口
            "domain_strategy": "ipv4_only"
        },
        {
            "type": "mixed",
            "listen": "::",
            "listen_port": 10809, // http和socks5监听端口
            "domain_strategy": "ipv4_only"
        }
    ],
    "outbounds": [
        {
            "type": "shadowsocks",
            "tag": "proxy",
            "server": "",  // 服务端的域名或IP
            "server_port": 10000,
            "method": "2022-blake3-aes-128-gcm",
            "password": "3P+xaSaFiXsrQ1KCr2Xvxg==", // 与服务端一致
            "udp_over_tcp": {
                "enabled": false,
                "version": 2
            }
        },
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        }
    ]
}
