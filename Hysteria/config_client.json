{
    "log": {
        "level": "info",
        "timestamp": true
    },
    "route": {
        "rules": [
            {
                "geosite": [
                    "cn",
                    "private"
                ],
                "geoip": [
                    "private"
                ],
                "outbound": "direct"
            }
        ]
    },
    "inbounds": [
        {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "::",
            "listen_port": 10000,
            "set_system_proxy": false
        }
    ],
    "outbounds": [
        {
            "type": "hysteria",
            "tag": "proxy",
            "server": "", // 服务端的域名或 IP
            "server_port": 443,
            "up_mbps": 50,
            "down_mbps": 150,
            "auth_str": "chika", // 与服务端一致
            "tls": {
                "enabled": true,
                "server_name": "", // 如果"server"填的是服务端的域名，可留空，如果"server"填的是服务端的IP，此处填服务端的证书中包含的域名
                "alpn": [
                    "h3"
                ]
            }
        },
        {
            "type": "direct",
            "tag": "direct"
        }
    ]
}
