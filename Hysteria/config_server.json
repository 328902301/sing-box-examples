{
    "inbounds": [
        {
            "type": "hysteria",
            "listen": "::",
            "listen_port": 443,
            "users": [
                {
                    "auth_str": "chika"
                }
            ],
            "tls": {
                "enabled": true,
                "alpn": [
                    "h3"
                ],
                "certificate_path": "/root/fullchain.cer",
                "key_path": "/root/private.key"
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct"
        }
    ]
}
